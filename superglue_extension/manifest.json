{
    "manifest_version": 3,
    "name": "SuperGlue",
    "permissions": [
        "tabs"
    ],
    "description": "Easily Backup And Restore Your Pinned Tabs.  Remember To Periodically Backup Your Tabs With A Single Click!",
    "version": "1.0",
    "action": {
        "default_popup": "superglue.html",
        "default_icon": "superglue.png"
    },
    "icons": {
        "16": "superglue.png",
        "32": "superglue.png",
        "48": "superglue.png",
        "128": "superglue.png"
    }
    
}