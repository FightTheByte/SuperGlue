{
    "manifest_version": 3,
    "name": "SuperGlue - Glue Pinned Tabs",
    "permissions": [
        "tabs",
        "bookmarks"
    ],
    "description": "Easily Save And Restore Your Pinned Tabs.",
    "version": "3.0",
    "action": {
        "default_popup": "superglue.html",
        "default_icon": "icons/icon128.png"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
    
}